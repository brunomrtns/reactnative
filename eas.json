{
  "cli": {
    "version": ">= 14.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "env": {
        "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org/"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "NPM_CONFIG_REGISTRY": "https://registry.npmjs.org/"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
